pipeline{

    agent {any}
    
    stages{

        stage("code"){
            steps{
              echo "This is cloning the code"
            }
        }
        stage("Build"){
            steps{
              echo "This is bulding the code"
            }
        }
        stage("Test"){
            steps{
              echo "This is testing the code"
            }
        }
        stage("Deploy"){
            steps{
              echo "This is deploying the code"
            }
        }
    }
}
